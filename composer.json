{
    "name": "asciisd/kyc-core",
    "description": "Core KYC (Know Your Customer) functionality for Laravel applications",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Asciisd",
            "email": "info@asciisd.com"
        }
    ],
    "require": {
        "php": "^8.2",
        "laravel/framework": "^12.0",
        "intervention/validation": "^4.0"
    },
    "require-dev": {
        "orchestra/testbench": "^10.0",
        "pestphp/pest": "^3.0",
        "pestphp/pest-plugin-laravel": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "Asciisd\\KycCore\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Asciisd\\KycCore\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Asciisd\\KycCore\\Providers\\KycServiceProvider"
            ]
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    }
}
